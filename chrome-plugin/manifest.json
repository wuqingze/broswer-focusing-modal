{
	"manifest_version": 3,
	"name": "浏览器专注模式",
	"version": "1.0",
	"description": "本插件可以是人冷静专注",
	"author": "wuqingze",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://*.twitter.com/*","https://twitter.com/home","https://www.twitter.com/*","https://www.twitter.com/", "https://www.guancha.cn/*","https://www.youtube.com/*"],
			"js": ["content-script.js"]
		}
	]
}
